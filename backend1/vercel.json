{
    "version": 2,
    "name": "server",
    "builds": [
      {
        "src": "/src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/index.js"
      },
      {
        "src": "/image/(.*)",
        "dest": "/public/image/$1"
      }
    ]
  }